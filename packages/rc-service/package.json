    "@reapit/utils-common": "workspace:packages/utils-common",
    "@reapit/utils-react": "workspace:packages/utils-react",
    "@sentry/browser": "^7.84.0",
    "dayjs": "^1.11.10",
    "just-debounce-it": "^1.5.0",
    "qrcode": "^1.5.3",
    "query-string": "5.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
