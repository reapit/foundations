    "@testing-library/react": "^14.1.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cross-env": "^6.0.3",
    "eslint": "8.54.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "jest-coverage-badges": "^1.1.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2",
    "vite": "^5.0.6"
