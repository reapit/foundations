{
  "name": "graphql-server",
  "version": "0.0.1",
  "private": true,
  "description": "GraphQL Server",
  "repository": "https://github.com/reapit/foundations.git",
  "license": "MIT",
  "author": "reapit global",
  "main": "dist/index.js",
  "scripts": {
    "start": "yarn build && yarn serverless offline start --stage local --printOutput",
    "test": "cross-env TZ=UTC jest --watch --colors --silent",
    "build": "node ./scripts/build-schema.js && prettier src/schema.graphql --write",
    "lint": "concurrently \"prettier src/**/*.graphql --write \"  \"eslint --cache --ext=ts,tsx src --fix\"",
    "release": "serverless deploy",
    "publish": "echo '...skipping...'",
    "conf": "yarn config-manager --namespace cloud --entity graphql-server --name local --mode fetch",
    "commit": "concurrently \"yarn test --coverage --no-cache --silent --forceExit --detectOpenHandles --runInBand --watch=false && jest-coverage-badges --input src/tests/coverage/coverage-summary.json --output src/tests/badges\" \"yarn lint\""
  },
  "dependencies": {
    "@reapit/elements": "workspace:packages/elements",
    "@reapit/utils": "workspace:packages/utils",
    "apollo-server-core": "^2.18.2",
    "apollo-server-lambda": "^2.18.2",
    "axios": "^0.21.0",
    "cookie": "0.4.1",
    "dataloader": "^2.0.0",
    "dayjs": "^1.10.4",
    "graphql": "14.7.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^0.7.1",
    "graphql-type-json": "^0.3.1",
    "https-proxy-agent": "5.0.0",
    "lodash.merge": "^4.6.2",
    "merge-graphql-schemas": "^1.7.6",
    "query-string": "^6.13.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "snyk": "^1.341.1",
    "tslib": "2.1.0",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@reapit/config-manager": "workspace:packages/config-manager",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/graphql-type-json": "^0.3.2",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.2",
    "@types/uuid": "^3.4.6",
    "babel-loader": "^8.0.6",
    "babel-runtime": "^6.26.0",
    "bufferutil": "^4.0.1",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "5.1.1",
    "cross-env": "^7.0.3",
    "eslint": "7.11.0",
    "jest": "^25.1.0",
    "jest-coverage-badges": "^1.1.2",
    "prettier": "^2.2.1",
    "serverless": "^2.23.0",
    "serverless-deployment-bucket": "^1.3.0",
    "serverless-offline": "^6.1.4",
    "serverless-webpack": "^5.3.2",
    "webpack": "^4.41.5",
    "webpack-node-externals": "^1.7.2"
  },
  "resolutions": {
    "graphql": "14.7.0"
  },
  "snyk": true
}
