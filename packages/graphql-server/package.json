{
  "name": "graphql-server",
  "version": "0.0.1",
  "private": true,
  "description": "GraphQL Server",
  "repository": "https://github.com/reapit/foundations.git",
  "license": "MIT",
  "author": "reapit global",
  "main": "dist/index.js",
  "scripts": {
    "start": "yarn build && yarn serverless offline start --runtime nodejs14.x --stage local",
    "test": "cross-env TZ=UTC jest --watch --colors --silent",
    "build": "node ./scripts/build-schema.js && prettier src/schema.graphql --write",
    "lint": "concurrently \"prettier src/**/*.graphql --write \"  \"eslint --cache --ext=ts,tsx src --fix\"",
    "release": "serverless deploy",
    "publish": "echo '...skipping...'",
    "conf": "yarn config-manager --namespace cloud --entity graphql-server --name local --mode fetch",
    "commit": "concurrently \"yarn test --coverage --no-cache --silent --forceExit --detectOpenHandles --runInBand --watch=false && jest-coverage-badges --input src/tests/coverage/coverage-summary.json --output src/tests/badges\" \"yarn lint\"",
    "prepublish": "npm run snyk-protect",
    "snyk-protect": "snyk-protect"
  },
  "dependencies": {
    "@reapit/utils": "workspace:packages/utils",
    "@snyk/protect": "latest",
    "apollo-server-core": "^3.0.1",
    "apollo-server-lambda": "^3.0.1",
    "axios": "^0.21.1",
    "cookie": "0.4.1",
    "dataloader": "^2.0.0",
    "dayjs": "^1.10.6",
    "graphql": "15.5.1",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^1.0.2",
    "graphql-type-json": "^0.3.2",
    "https-proxy-agent": "5.0.0",
    "lodash.merge": "^4.6.2",
    "merge-graphql-schemas": "^1.7.8",
    "query-string": "^6.14.1",
    "serialize-error": "^7.0.1",
    "tslib": "2.3.1",
    "uuid": "^3.4.0",
    "vue": "^2.6.14",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@reapit/config-manager": "workspace:packages/config-manager",
    "@reapit/ts-scripts": "workspace:packages/ts-scripts",
    "@reapit/ts-types": "workspace:packages/ts-types",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "babel-loader": "^8.2.2",
    "babel-runtime": "^6.26.0",
    "bufferutil": "^4.0.3",
    "concurrently": "^6.2.0",
    "copy-webpack-plugin": "9.0.1",
    "cross-env": "^7.0.3",
    "eslint": "7.31.0",
    "eslint-plugin-prettier": "^3.4.0",
    "graphql-tag": "^2.12.5",
    "jest": "^27.0.6",
    "jest-coverage-badges": "^1.1.2",
    "prettier": "^2.3.2",
    "serverless": "^2.52.0",
    "serverless-deployment-bucket": "^1.5.1",
    "serverless-offline": "^7.0.0",
    "serverless-webpack": "^5.5.1",
    "webpack": "^5.45.1",
    "webpack-node-externals": "^3.0.0"
  },
  "snyk": true
}
