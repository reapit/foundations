{
  "name": "graphql-server",
  "version": "0.0.1",
  "private": true,
  "description": "GraphQL Server",
  "repository": "https://github.com/reapit/foundations.git",
  "license": "MIT",
  "author": "reapit global",
  "main": "dist/index.js",
  "scripts": {
    "start": "PLATFORM_API_BASE_URL=https://platform.dev.paas.reapit.cloud ts-node-dev -r tsconfig-paths/register src/offline",
    "test": "cross-env TZ=UTC jest --watch --colors --silent",
    "build:schema": "node ./scripts/build-schema.js && prettier src/schema.graphql --write",
    "build": "yarn build:schema && tsc && cp src/schema.graphql dist/graphql-server/src/schema.graphql",
    "bundle": "ts-bundler",
    "lint": "concurrently \"prettier src/**/*.graphql --write \"  \"eslint --cache --ext=ts,tsx src --fix\"",
    "publish": "echo '...skipping...'",
    "conf": "yarn config-manager --namespace cloud --entity graphql-server --name local --mode fetch",
    "commit": "concurrently \"yarn test --coverage --no-cache --silent --forceExit --detectOpenHandles --runInBand --watch=false && jest-coverage-badges --input src/tests/coverage/coverage-summary.json --output src/tests/badges\" \"yarn lint\"",
    "prepublish": "npm run snyk-protect",
    "snyk-protect": "snyk-protect",
    "release": "rpt-cdk deploy",
    "synth": "rpt-cdk synth"
  },
  "dependencies": {
    "@reapit/utils-node": "workspace:packages/utils-node",
    "@snyk/protect": "^1.793.0",
    "apollo-server": "^3.5.0",
    "apollo-server-core": "^3.5.0",
    "apollo-server-lambda": "^3.5.0",
    "axios": "^0.24.0",
    "axios-mock-adapter": "^1.20.0",
    "axios-retry": "^3.2.4",
    "cookie": "0.4.1",
    "dataloader": "^2.0.0",
    "dayjs": "^1.10.7",
    "graphql": "15.8.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^1.0.2",
    "graphql-type-json": "^0.3.2",
    "https-proxy-agent": "5.0.0",
    "lodash.merge": "^4.6.2",
    "merge-graphql-schemas": "^1.7.8",
    "query-string": "^6.14.1",
    "serialize-error": "^7.0.1",
    "tslib": "2.3.1",
    "uuid": "^3.4.0",
    "vue": "^2.6.14",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.5",
    "@reapit/config-manager": "workspace:packages/config-manager",
    "@reapit/ts-bundler": "workspace:packages/ts-bundler",
    "@reapit/ts-scripts": "workspace:packages/ts-scripts",
    "@reapit/ts-types": "workspace:packages/ts-types",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "bufferutil": "^4.0.5",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "eslint": "8.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "graphql-tag": "^2.12.6",
    "jest": "^27.4.5",
    "jest-coverage-badges": "^1.1.2",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.3.5"
  },
  "resolutions": {
    "logform": "2.3.0"
  },
  "snyk": true
}
