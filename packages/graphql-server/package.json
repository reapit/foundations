{
  "name": "graphql-server",
  "version": "1.3.5",
  "private": true,
  "description": "GraphQL Server",
  "repository": "https://github.com/reapit/foundations.git",
  "license": "MIT",
  "author": "reapit global",
  "main": "dist/index.js",
  "scripts": {
    "start": "PLATFORM_API_BASE_URL=https://platform.dev.paas.reapit.cloud ts-node-dev -r tsconfig-paths/register src/offline",
    "test": "cross-env TZ=UTC jest --watch --colors --silent",
    "build:schema": "node ./scripts/build-schema.js && prettier src/schema.graphql --write",
    "build": "yarn build:schema && tsc && cp src/schema.graphql dist/graphql-server/src/schema.graphql",
    "bundle": "ts-bundler",
    "lint": "concurrently \"prettier src/**/*.graphql --write \"  \"eslint --cache --ext=ts,tsx src\"",
    "check": "echo '...skipping...'",
    "publish": "echo '...skipping...'",
    "conf": "yarn config-manager --namespace cloud --entity graphql-server --name local --mode fetch",
    "commit": "yarn test --coverage --no-cache --silent --forceExit --detectOpenHandles --runInBand --watch=false && jest-coverage-badges --input src/tests/coverage/coverage-summary.json --output src/tests/badges && yarn lint --fix && yarn check",
    "prepublish": "npm run snyk-protect",
    "snyk-protect": "snyk-protect",
    "deploy": "rpt-cdk deploy",
    "release": "echo '...skipping...'",
    "synth": "rpt-cdk synth"
  },
  "dependencies": {
    "@reapit/utils-node": "workspace:packages/utils-node",
    "@snyk/protect": "^1.1032.0",
    "apollo-server": "^3.10.3",
    "apollo-server-core": "^3.10.3",
    "apollo-server-lambda": "^3.10.3",
    "axios": "^0.27.2",
    "axios-mock-adapter": "^1.21.2",
    "axios-retry": "^3.3.1",
    "cookie": "0.5.0",
    "dataloader": "^2.1.0",
    "dayjs": "^1.11.5",
    "express": "^4.18.2",
    "graphql": "^15.8.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^1.0.2",
    "graphql-type-json": "^0.3.2",
    "https-proxy-agent": "5.0.1",
    "lodash.merge": "^4.6.2",
    "merge-graphql-schemas": "^1.7.8",
    "query-string": "^6.14.1",
    "serialize-error": "^7.0.1",
    "tslib": "2.4.1",
    "uuid": "^3.4.0",
    "vue": "^2.7.13",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@reapit/config-manager": "workspace:packages/config-manager",
    "@reapit/ts-bundler": "workspace:packages/ts-bundler",
    "@reapit/ts-scripts": "workspace:packages/ts-scripts",
    "@reapit/ts-types": "workspace:packages/ts-types",
    "@types/graphql": "^14.5.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "bufferutil": "^4.0.6",
    "concurrently": "^6.5.1",
    "cross-env": "^7.0.3",
    "eslint": "8.25.0",
    "eslint-plugin-prettier": "^4.2.1",
    "graphql-tag": "^2.12.6",
    "jest": "^28.1.3",
    "jest-coverage-badges": "^1.1.2",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.8.4"
  },
  "resolutions": {
    "logform": "2.3.0"
  },
  "snyk": true
}
