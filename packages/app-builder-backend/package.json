{
  "name": "@reapit/app-builder-backend",
  "version": "0.0.1",
  "description": "Common Package for Reapit Foundation",
  "repository": {
    "type": "git",
    "url": "git+git@github.com:reapit/foundations.git"
  },
  "license": "MIT",
  "author": "Reapit Ltd",
  "main": "./src/index.ts",
  "scripts": {
    "start": "DYNAMODB_ENDPOINT=http://localhost:8000 ts-node-dev .",
    "test": "echo '...skipping...'",
    "build": "cp config.json src && tsc",
    "lint": "eslint --cache --ext=ts,tsx,js src --fix",
    "release": "DOCKER_BUILDKIT=1 cdk deploy --require-approval never",
    "publish": "yarn npm publish --access public",
    "conf": "yarn config-manager --namespace cloud --entity app-builder --mode fetch",
    "synth": "cdk synth",
    "ddb": "docker run -d -p 8000:8000 amazon/dynamodb-local",
    "commit": "concurrently \"yarn test --coverage --no-cache --silent --forceExit --detectOpenHandles --runInBand --watch=false\" \"yarn lint\""
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.27.0",
    "@reapit/app-builder": "workspace:packages/app-builder",
    "@reapit/cra-template-foundations": "workspace:packages/cra-template-foundations",
    "@reapit/foundations-ts-definitions": "workspace:packages/foundations-ts-definitions",
    "@reapit/ts-scripts": "workspace:packages/ts-scripts",
    "@reapit/utils-common": "workspace:packages/utils-common",
    "@types/luxon": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "apollo-server-core": "^3.3.0",
    "apollo-server-express": "^3.3.0",
    "apollo-server-lambda": "^3.3.0",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "eslint": "7.31.0",
    "eslint-plugin-prettier": "^3.4.0",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "graphql": "^15.5.1",
    "graphql-type-json": "^0.3.2",
    "jszip": "^3.7.1",
    "luxon": "^2.0.2",
    "node-fetch": "^2.6.1",
    "project-name-generator": "^2.1.9",
    "reflect-metadata": "^0.1.13",
    "slugify": "^1.6.0",
    "type-graphql": "^1.1.1",
    "typescript": "^4.3.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigateway": "1.116.0",
    "@aws-cdk/aws-dynamodb": "1.116.0",
    "@aws-cdk/aws-lambda": "1.116.0",
    "@aws-cdk/core": "1.116.0",
    "@reapit/config-manager": "workspace:packages/config-manager",
    "@reapit/ts-types": "workspace:packages/ts-types",
    "@types/fs-extra": "^9.0.12",
    "@types/jszip": "^3.4.1",
    "aws-cdk": "1.116.0",
    "babel-jest": "^27.0.6",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "jest": "^27.0.6",
    "jest-coverage-badges": "^1.1.2",
    "ts-node-dev": "^1.1.8"
  }
}
