{
  "name": "@reapit/app-builder-backend",
  "version": "0.0.1",
  "description": "Common Package for Reapit Foundation",
  "repository": {
    "type": "git",
    "url": "git+git@github.com:reapit/foundations.git"
  },
  "license": "MIT",
  "author": "Reapit Ltd",
  "main": "./src/index.ts",
  "scripts": {
    "start": "DYNAMODB_ENDPOINT=http://localhost:8000 ts-node-dev .",
    "test": "cp config.json src && cross-env TZ=UTC jest --no-cache --watch --colors --silent=false",
    "build": "cp config.json src && tsc",
    "bundle": "ts-bundler",
    "lint": "eslint --cache --ext=ts,tsx,js src",
    "release": "rpt-cdk deploy",
    "release:watch": "rpt-cdk watch",
    "release:destroy": "rpt-cdk destroy",
    "check": "tsc --noEmit --skipLibCheck",
    "publish": "echo '...skipping...'",
    "conf": "yarn config-manager --namespace cloud --entity app-builder --name local --mode fetch",
    "synth": "rpt-cdk synth",
    "ddb": "docker run -d -p 8000:8000 amazon/dynamodb-local",
    "commit": "yarn test --coverage --no-cache --silent --forceExit --detectOpenHandles --runInBand --watch=false && yarn lint --fix && yarn check"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.27.0",
    "@graphql-tools/stitch": "^8.4.3",
    "@reapit/app-builder": "workspace:packages/app-builder",
    "@reapit/cra-template-foundations": "workspace:packages/cra-template-foundations",
    "@reapit/foundations-ts-definitions": "workspace:packages/foundations-ts-definitions",
    "@reapit/ts-scripts": "workspace:packages/ts-scripts",
    "@reapit/utils-common": "workspace:packages/utils-common",
    "@types/luxon": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "apollo-server-core": "^3.5.0",
    "apollo-server-express": "^3.5.0",
    "apollo-server-lambda": "^3.5.0",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "eslint": "^8.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "get-graphql-from-jsonschema": "^8.0.16",
    "graphql": "^15.8.0",
    "graphql-type-json": "^0.3.2",
    "jszip": "^3.7.1",
    "luxon": "^2.2.0",
    "node-fetch": "2",
    "pluralize": "^8.0.0",
    "project-name-generator": "^2.1.9",
    "reflect-metadata": "^0.1.13",
    "slugify": "^1.6.3",
    "type-graphql": "^1.1.1",
    "typescript": "^4.3.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@reapit/config-manager": "workspace:packages/config-manager",
    "@reapit/ts-bundler": "workspace:packages/ts-bundler",
    "@reapit/ts-types": "workspace:packages/ts-types",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.4.0",
    "@types/jszip": "^3.4.1",
    "adm-zip": "^0.5.9",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "fast-deep-equal": "^3.1.3",
    "fetch-mock-jest": "^1.5.1",
    "jest": "^27.4.5",
    "jest-coverage-badges": "^1.1.2",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8"
  }
}
