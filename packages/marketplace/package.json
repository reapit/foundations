{
  "name": "marketplace",
  "version": "1.0.0",
  "description": "Marketplace for PAAS platform",
  "main": "./src/index.ts",
  "repository": "git@github.com:reapit/app-store.git",
  "author": "Will McVay <wmcvay@reapit.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --hot --color --mode development",
    "test": "cross-env TZ=UTC jest --watch --colors --silent",
    "build": "rimraf public/dist && NODE_ENV=production webpack",
    "lint": "eslint --cache --ext=ts,tsx src --fix",
    "release": "serverless deploy",
    "publish": "echo '...skipping...'",
    "conf": "yarn config-manager --namespace cloud --entity marketplace --name local --mode fetch",
    "commit": "concurrently \"yarn test --coverage --no-cache --silent --forceExit --detectOpenHandles --runInBand --watch=false && jest-coverage-badges --input src/tests/coverage/coverage-summary.json --output src/tests/badges\" \"yarn lint\""
  },
  "dependencies": {
    "@react-hook/resize-observer": "^1.1.0",
    "@reapit/connect-session": "workspace:packages/connect-session",
    "@reapit/elements": "workspace:packages/elements",
    "@reapit/utils": "workspace:packages/utils",
    "amazon-cognito-identity-js": "^4.5.11",
    "chart.js": "^2.9.3",
    "dayjs": "^1.10.4",
    "diff": "^4.0.1",
    "lodash.isequal": "^4.5.0",
    "lodash.orderby": "^4.6.0",
    "react": "^16.12.0",
    "react-chartjs-2": "^2.8.0",
    "react-dom": "^16.12.0",
    "react-infinite-scroll-component": "^5.0.5",
    "react-responsive": "8.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-transition-group": "^4.4.1",
    "snyk": "^1.341.1"
  },
  "devDependencies": {
    "@reapit/config-manager": "workspace:packages/config-manager",
    "@reapit/foundations-ts-definitions": "workspace:packages/foundations-ts-definitions",
    "@types/nanoid": "^2.1.0",
    "concurrently": "^5.3.0",
    "cross-env": "^6.0.3",
    "eslint": "7.11.0",
    "jest": "^26.6.1",
    "jest-coverage-badges": "^1.1.2",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^3.0.2",
    "serverless": "^2.23.0",
    "serverless-s3-deploy": "^0.10.0",
    "serverless-s3-remover": "^0.6.0",
    "serverless-single-page-app-plugin": "^1.0.2",
    "webpack": "^4.41.5",
    "webpack-dev-server": "^3.11.0"
  },
  "browserslist": [
    "> 0.5%",
    "not IE 11",
    "chrome 69"
  ],
  "snyk": true
}
