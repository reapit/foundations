    "@sentry/browser": "^7.84.0",
    "powerbi-client": "^2.22.4",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-router": "^6.20.0",
    "react-router-dom": "^6.20.0",
    "snyk": "^1.1259.0"
  },
  "devDependencies": {
    "@linaria/shaker": "^5.0.3",
    "@reapit/config-manager": "workspace:packages/config-manager",
