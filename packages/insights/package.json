    "babel-loader": "^9.1.3",
    "concurrently": "^6.5.1",
    "cross-env": "^6.0.3",
    "eslint": "8.56.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-coverage-badges": "^1.1.2",
    "node-polyfill-webpack-plugin": "^3.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  }
