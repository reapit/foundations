{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["es2019"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": false,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
    "strict": true,
    "target": "es5",
    "sourceMap": true,
    "sourceRoot": "/",
    "inlineSources": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "baseUrl": "./",
    "typeRoots": ["./node_modules/@types", "../../node_modules/@types"],
    "paths": {
      
    }
  },
  "exclude": ["public", "dist", "scripts", "**/node_modules", "coverage", "archive/**/*", "**/__tests__/**/*", "cdk/**/*"]
}
