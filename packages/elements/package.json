{
  "name": "@reapit/elements",
  "version": "1.1.0",
  "description": "The pre-cursor to the next major version of Elements (A collection of React components and utilities for building apps for Reapit Marketplace)",
  "homepage": "https://github.com/reapit/foundations#readme",
  "bugs": {
    "url": "https://github.com/reapit/foundations/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reapit/foundations.git"
  },
  "license": "MIT",
  "author": "Will McVay <wmcvay@reapit.com>",
  "main": "dist/index.js",
  "umd:main": "dist/elements.umd.production.js",
  "module": "dist/elements.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src/tests/badges"
  ],
  "scripts": {
    "build:prod": "concurrently \"cross-env NODE_ENV=production && rimraf dist && tsdx build --format=cjs,esm,umd\" \"cross-env NODE_ENV=production && rimraf public/dist && build-storybook -o public/dist\"",
    "check:ci": "concurrently \"tsc --noEmit\" \"eslint --cache --ext=ts,tsx src\"",
    "check:fix": "concurrently \"tsc --noEmit\" \"eslint --cache --ext=ts,tsx src --fix\"",
    "fetch-config": "yarn config-manager --namespace cloud --entity elements --name local --mode fetch",
    "prepublish": "npm run snyk-protect",
    "release:development": "serverless deploy --stage dev",
    "release:production": "serverless deploy --stage prod",
    "snyk-protect": "snyk protect",
    "start:dev": "start-storybook -p 6006",
    "start:prod": "serve -l 8080 public/dist",
    "test:ci": "cross-env TZ=UTC jest --ci --colors --coverage --silent --forceExit --detectOpenHandles --runInBand",
    "test:dev": "cross-env TZ=UTC jest --watch --silent",
    "test:update-badges": "yarn test:ci && jest-coverage-badges --input src/tests/coverage/coverage-summary.json --output src/tests/badges"
  },
  "browserslist": [
    "> 0.5%",
    "not IE 11",
    "chrome 69"
  ],
  "dependencies": {
    "bulma": "^0.9.0",
    "file-saver": "^2.0.2",
    "formik": "^2.0.4",
    "hardtack": "^4.1.0",
    "himalaya": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "linaria": "^1.4.0-beta.10",
    "papaparse": "^5.1.1",
    "pell": "^1.0.6",
    "prop-types": "^15.7.2",
    "rc-dialog": "^8.0.1",
    "rc-notification": "^4.0.0",
    "rc-select": "^10.1.8",
    "rc-tooltip": "^4.0.3",
    "react-datasheet": "^1.4.0",
    "react-datepicker": "^2.9.6",
    "react-google-map": "^3.1.1",
    "react-google-maps-loader": "4.3.0",
    "react-icons": "^3.7.0",
    "react-image-crop": "8.6.6",
    "react-table": "^7.3.0",
    "react-transition-group": "^4.4.0",
    "snyk": "^1.338.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.10.1",
    "@babel/runtime": "7.10.2",
    "@reapit/config-manager": "workspace:packages/config-manager",
    "@reapit/foundations-ts-definitions": "2020-02-13",
    "@rollup/plugin-babel": "^5.0.3",
    "@storybook/addon-actions": "^6.1.15",
    "@storybook/addon-essentials": "^6.1.15",
    "@storybook/addon-storysource": "^6.1.15",
    "@storybook/addons": "^6.1.15",
    "@storybook/cli": "^6.1.15",
    "@storybook/react": "^6.1.15",
    "@storybook/theming": "^6.1.15",
    "@types/googlemaps": "^3.37.5",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/rc-select": "^5.9.34",
    "@types/react-datepicker": "^3.0.3",
    "@types/react-google-maps-loader": "^4.2.1",
    "@types/react-transition-group": "^4.4.0",
    "@whitespace/storybook-addon-html": "^4.2.0",
    "concurrently": "^5.3.0",
    "dashify": "2.0.0",
    "eslint": "7.11.0",
    "react-is": "^16.13.1",
    "rollup": "1.27.13",
    "rollup-plugin-scss": "^1.0.2",
    "rollup-plugin-typescript2": "^0.27.1"
  },
  "peerDependencies": {
    "dayjs": "^1.8.19",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "snyk": true
}
