{
  "name": "@reapit/elements",
  "version": "3.0.0-alpha.1",
  "description": "A collection of React components and utilities for building apps for Reapit Marketplace",
  "homepage": "https://github.com/reapit/foundations#readme",
  "bugs": {
    "url": "https://github.com/reapit/foundations/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reapit/foundations.git"
  },
  "license": "MIT",
  "author": "Will McVay <wmcvay@reapit.com>",
  "main": "dist/index.js",
  "umd:main": "dist/elements.umd.production.js",
  "module": "dist/elements.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/",
    "src/tests/badges"
  ],
  "scripts": {
    "build": "yarn build:dist && yarn build:storybook",
    "build:storybook": "cross-env NODE_ENV=production && build-storybook -o public/dist",
    "build:dist": "cross-env NODE_ENV=production && rimraf public/dist && tsdx build --format=cjs,esm,umd",
    "commit": "concurrently \"yarn test --coverage --no-cache --silent --forceExit --detectOpenHandles --runInBand --watch=false && jest-coverage-badges --input src/tests/coverage/coverage-summary.json --output src/tests/badges\" \"yarn lint\"",
    "conf": "yarn config-manager --namespace cloud --entity elements --name local --mode fetch",
    "lint": "eslint --cache --ext=ts,tsx src --fix",
    "publish": "yarn npm publish --access public",
    "release": "serverless deploy",
    "start": "start-storybook -p 6006",
    "test": "cross-env TZ=UTC jest --watch --colors --silent"
  },
  "browserslist": [
    "> 0.5%",
    "not IE 11",
    "chrome 69"
  ],
  "dependencies": {
    "linaria": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-transform-runtime": "7.13.8",
    "@babel/runtime": "7.13.8",
    "@mdx-js/react": "^1.6.22",
    "@reapit/config-manager": "workspace:packages/config-manager",
    "@reapit/ts-scripts": "workspace:packages/ts-scripts",
    "@reapit/ts-types": "workspace:packages/ts-types",
    "@rollup/plugin-babel": "^5.3.0",
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addon-docs": "^6.1.20",
    "@storybook/addon-essentials": "^6.1.20",
    "@storybook/addon-links": "^6.1.20",
    "@storybook/addon-storysource": "^6.1.20",
    "@storybook/addons": "^6.1.20",
    "@storybook/cli": "^6.1.20",
    "@storybook/csf": "^0.0.1",
    "@storybook/react": "^6.1.20",
    "@storybook/theming": "^6.1.20",
    "@svgr/rollup": "^5.5.0",
    "@whitespace/storybook-addon-html": "^4.2.0",
    "babel-loader": "^8.2.2",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "dashify": "2.0.0",
    "eslint": "7.23.0",
    "jest": "^26.6.3",
    "jest-coverage-badges": "^1.1.2",
    "move-file-cli": "^2.0.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-is": "^16.13.1",
    "rimraf": "^3.0.2",
    "rollup": "1.32.1",
    "rollup-plugin-scss": "^1.0.2",
    "rollup-plugin-typescript2": "^0.27.3",
    "serverless": "^2.28.0",
    "serverless-s3-deploy": "^0.10.0",
    "serverless-s3-remover": "^0.6.0",
    "serverless-single-page-app-plugin": "^1.0.2",
    "snyk": "^1.463.0",
    "tsdx": "^0.14.1",
    "tslib": "^1.14.1",
    "typescript": "^4.0.3",
    "webpack": "^4.46.0"
  },
  "peerDependencies": {
    "dayjs": "^1.8.19",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "snyk": true
}
