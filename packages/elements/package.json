{
  "name": "@reapit/elements",
  "version": "2.0.1",
  "description": "A collection of React components and utilities for building apps for Reapit Marketplace",
  "homepage": "https://github.com/reapit/foundations#readme",
  "bugs": {
    "url": "https://github.com/reapit/foundations/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reapit/foundations.git"
  },
  "license": "MIT",
  "author": "Will McVay <wmcvay@reapit.com>",
  "main": "dist/index.js",
  "umd:main": "dist/elements.umd.production.js",
  "module": "dist/elements.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src/tests/badges"
  ],
  "scripts": {
    "build": "yarn build:v3 && yarn build:v2",
    "build:v2": "concurrently \"cross-env NODE_ENV=production && rimraf dist && tsdx build --format=cjs,esm,umd\" \"cross-env NODE_ENV=production && rimraf public/dist && build-storybook -o public/dist\"",
    "build:v3": "cross-env NODE_ENV=production && rimraf v3 && rimraf dist && tsdx build --entry src/v3.tsx --name v3 --format=cjs,esm,umd && move-file dist/ v3/",
    "commit": "concurrently \"yarn test --coverage --no-cache --silent --forceExit --detectOpenHandles --runInBand --watch=false && jest-coverage-badges --input src/tests/coverage/coverage-summary.json --output src/tests/badges\" \"yarn lint\"",
    "conf": "yarn config-manager --namespace cloud --entity elements --name local --mode fetch",
    "lint": "eslint --cache --ext=ts,tsx src --fix",
    "publish": "yarn npm publish --access public",
    "release": "serverless deploy",
    "start": "start-storybook -p 6006",
    "test": "cross-env TZ=UTC jest --watch --colors --silent"
  },
  "browserslist": [
    "> 0.5%",
    "not IE 11",
    "chrome 69"
  ],
  "dependencies": {
    "@svgr/rollup": "^5.5.0",
    "@svgr/webpack": "^5.5.0",
    "babel-loader": "^8.2.2",
    "bulma": "^0.9.2",
    "file-saver": "^2.0.5",
    "formik": "^2.2.6",
    "hardtack": "^4.1.2",
    "himalaya": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "linaria": "^2.0.2",
    "move-file-cli": "^2.0.0",
    "papaparse": "^5.3.0",
    "pell": "^1.0.6",
    "rc-dialog": "^8.5.1",
    "rc-notification": "^4.5.4",
    "rc-select": "^10.5.1",
    "rc-tooltip": "^4.2.3",
    "react-datasheet": "^1.4.9",
    "react-datepicker": "^2.16.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-google-map": "^3.1.1",
    "react-google-maps-loader": "4.3.0",
    "react-icons": "^3.11.0",
    "react-image-crop": "8.6.6",
    "react-table": "^7.6.3",
    "react-transition-group": "^4.4.1",
    "snyk": "^1.463.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-transform-runtime": "7.13.8",
    "@babel/runtime": "7.13.8",
    "@mdx-js/react": "^1.6.22",
    "@reapit/config-manager": "workspace:packages/config-manager",
    "@reapit/foundations-ts-definitions": "workspace:packages/foundations-ts-definitions",
    "@rollup/plugin-babel": "^5.3.0",
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addon-docs": "^6.1.20",
    "@storybook/addon-essentials": "^6.1.20",
    "@storybook/addon-links": "^6.1.20",
    "@storybook/addon-storysource": "^6.1.20",
    "@storybook/addons": "^6.1.20",
    "@storybook/cli": "^6.1.20",
    "@storybook/react": "^6.1.20",
    "@storybook/theming": "^6.1.20",
    "@whitespace/storybook-addon-html": "^4.2.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "dashify": "2.0.0",
    "eslint": "7.23.0",
    "fs-extra": "^9.1.0",
    "jest": "^26.6.3",
    "jest-coverage-badges": "^1.1.2",
    "react-is": "^16.13.1",
    "rimraf": "^3.0.2",
    "rollup": "1.32.1",
    "rollup-plugin-scss": "^1.0.2",
    "rollup-plugin-typescript2": "^0.27.3",
    "serverless": "^2.28.0",
    "serverless-s3-deploy": "^0.10.0",
    "serverless-s3-remover": "^0.6.0",
    "serverless-single-page-app-plugin": "^1.0.2",
    "tsdx": "^0.14.1",
    "tslib": "^1.14.1",
    "typescript": "^4.0.3",
    "webpack": "^4.46.0"
  },
  "peerDependencies": {
    "dayjs": "^1.8.19",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "snyk": true
}
