    "@tsconfig/svelte": "^1.0.2",
    "@types/googlemaps": "^3.39.3",
    "@types/lodash.pickby": "^4.6.6",
    "chokidar": "^3.4.0",
    "cross-env": "^7.0.2",
    "jest-emotion": "^10.0.32",
