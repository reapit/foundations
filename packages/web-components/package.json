    "svelte:check": "npx svelte-check",
    "fetch-config": "yarn config-manager fetchConfig web-components",
    "release:dev": "node ./scripts/release-serverless.js --stage dev && node ../../scripts/release/release-dev.js web-components reapit-web-components",
    "release:development": "node ./scripts/release-serverless.js --stage dev",
    "release:production": "node ./scripts/release-serverless.js --stage prod",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "snyk": true
