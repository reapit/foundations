    "@reapit/api-key-verify": "workspace:packages/api-key-verify",
    "@reapit/connect-session": "workspace:packages/connect-session",
    "@reapit/utils-common": "workspace:packages/utils-common",
    "@reapit/utils-node": "workspace:packages/utils-node",
    "adm-zip": "^0.5.6",
    "aws-cdk": "1.124.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "mime-types": "^2.1.32",
    "mysql": "^2.18.1",
    "nestjs-typeorm-paginate": "^2.5.3",
    "original-fs": "^1.1.0",
    "project-name-generator": "^2.1.9",
    "pusher": "^5.0.0",
    "rimraf": "^3.0.2",
    "typeorm": "^0.2.34",
    "typeorm-aurora-data-api-driver": "^2.2.0",
    "yaml": "next"
  },
  "scripts": {
    "start": "echo '...skipping...'",
    "test": "echo '...skipping...'",
    "build": "webpack --config webpack.config.js",
    "lint": "eslint --cache --ext=ts,tsx,js src cdk/lib --fix",
    "deploy": "cdk synth",
    "release": "echo 'yarn db-conf && yarn migration:run && yarn deploy'",
    "publish": "echo '...skipping...'",
    "conf": "yarn config-manager --namespace cloud --entity deployment-service --name local --mode fetch",
    "db-conf": "yarn config-manager --namespace cloud --entity deployment-service --name db-config --mode fetch --file-path ormconfig.json",
    "migration:generate": "ts-node -P ./tsconfig.json ../../node_modules/.bin/typeorm migration:generate",
    "migration:run": "ts-node -P ./tsconfig.json ../../node_modules/.bin/typeorm migration:run",
    "migration:revert": "ts-node -P ./tsconfig.json ../../node_modules/.bin/typeorm migration:revert",
    "cdk:build": "tsc -p cdk/tsconfig.json"
  },
  "devDependencies": {
    "@aws-cdk/assets": "1.124.0",
    "@aws-cdk/aws-apigateway": "1.124.0",
    "@aws-cdk/aws-codebuild": "1.124.0",
