{
  "name": "deployment-service",
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.281.0",
    "@aws-sdk/client-route-53": "^3.281.0",
    "@nestjs/axios": "^0.1.1",
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.5",
    "@nestjs/microservices": "^8.4.7",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/typeorm": "^8.1.4",
    "@octokit/app": "^12.0.7",
    "@reapit/api-key-verify": "workspace:packages/api-key-verify",
    "@reapit/connect-session": "workspace:packages/connect-session",
    "@reapit/utils-nest": "workspace:packages/utils-nest",
    "@reapit/utils-node": "workspace:packages/utils-node",
    "adm-zip": "^0.5.10",
    "atlassian-jwt": "^2.0.2",
    "aws-serverless-express": "^3.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "jwt-decode": "^3.1.2",
    "mime-types": "^2.1.35",
    "mysql": "^2.18.1",
    "nestjs-typeorm-paginate": "^4.0.3",
    "original-fs": "^1.2.0",
    "passport-jwt": "^4.0.1",
    "project-name-generator": "^2.1.9",
    "pusher": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0",
    "typeorm": "^0.3.12",
    "yaml": "^2.2.1"
  },
  "main": "./src/index.ts",
  "scripts": {
    "start": "NODE_ENV=local nodemon src/local-http.ts",
    "start:sqs": "NODE_ENV=local nodemon src/sqs.ts",
    "start:sns": "NODE_ENV=local nodemon src/sns.ts",
    "test": "cross-env jest --watch --colors",
    "build": "tsc -p .",
    "lint": "eslint --cache --ext=ts,tsx,js src cdk/lib",
    "check": "tsc --noEmit --skipLibCheck",
    "deploy": "rpt-cdk deploy cdk/cdk.ts",
    "release": "echo '...skipping...'",
    "synth": "rpt-cdk synth cdk/cdk.ts",
    "bundle": "ts-bundler",
    "release:watch": "rpt-cdk watch cdk/cdk.ts",
    "release:destroy": "rpt-cdk destroy cdk/cdk.ts",
    "publish": "echo '...skipping...'",
    "conf": "yarn config-manager --namespace cloud --entity deployment-service --name local --mode fetch",
    "migration:generate": "typeorm-ts-node-common-js -d orm-datasource.tstypeorm migration:generate",
    "migration:run": " typeorm-ts-node-commonjs -d orm-datasource.ts migration:run",
    "migration:create": "typeorm-ts-node-common-js -d orm-datasource.tstypeorm migration:create",
    "migration:revert": "typeorm-ts-node-common-js -d orm-datasource.tstypeorm migration:revert"
  },
  "devDependencies": {
    "@nestjs/testing": "^9.3.9",
    "@reapit/config-manager": "workspace:packages/config-manager",
    "@reapit/ts-bundler": "workspace:packages/ts-bundler",
    "@reapit/ts-scripts": "workspace:packages/ts-scripts",
    "@types/adm-zip": "^0.5.0",
    "@types/aws-serverless-express": "^3.3.5",
    "@types/mime-types": "^2.1.1",
    "@types/passport-jwt": "^3.0.8",
    "@types/project-name-generator": "^2.1.1",
    "@types/rimraf": "^3.0.2",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "concurrently": "^6.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.35.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "mime": "2.6.0",
    "nodemon": "^2.0.20",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "version": "2.1.5"
}
