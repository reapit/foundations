{
  "name": "deployment-service",
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.535.0",
    "@aws-sdk/client-route-53": "^3.536.0",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^2.3.4",
    "@nestjs/core": "^9.4.3",
    "@nestjs/microservices": "^8.4.7",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/typeorm": "^8.1.4",
    "@octokit/app": "^14.0.2",
    "@reapit/api-key-verify": "workspace:packages/api-key-verify",
    "@reapit/connect-session": "workspace:packages/connect-session",
    "@reapit/utils-authorizer": "workspace:^",
    "@reapit/utils-nest": "workspace:packages/utils-nest",
    "@reapit/utils-node": "workspace:packages/utils-node",
    "adm-zip": "^0.5.12",
    "atlassian-jwt": "^2.0.3",
    "aws-cdk": "^2.133.0",
    "aws-serverless-express": "^3.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "jwt-decode": "^3.1.2",
    "mime-types": "^2.1.35",
    "mysql": "^2.18.1",
    "nestjs-typeorm-paginate": "^4.0.4",
    "original-fs": "^1.2.0",
    "passport-jwt": "^4.0.1",
    "project-name-generator": "^2.1.9",
    "pusher": "^5.2.0",
    "reflect-metadata": "^0.1.14",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "yaml": "^2.4.1"
  },
  "main": "./src/index.ts",
  "scripts": {
    "start": "NODE_ENV=local nodemon src/local-http.ts",
    "start:sqs": "NODE_ENV=local nodemon src/sqs.ts",
    "start:sns": "NODE_ENV=local nodemon src/sns.ts",
    "test": "cross-env jest --watch --colors",
    "build": "tsup",
    "lint": "eslint --cache --ext=ts,tsx,js src cdk/lib migrations",
    "check": "tsc --noEmit --skipLibCheck",
    "deploy": "rpt-cdk deploy cdk/cdk.ts",
    "release": "echo '...skipping...'",
    "synth": "rpt-cdk synth cdk/cdk.ts",
    "bundle": "tsup-zip deployment-service",
    "release:watch": "rpt-cdk watch cdk/cdk.ts",
    "release:destroy": "rpt-cdk destroy cdk/cdk.ts",
    "publish": "echo '...skipping...'",
    "conf": "yarn config-manager --namespace cloud --entity deployment-service --name local --mode fetch",
    "migration:generate": "typeorm-ts-node-commonjs -d orm-datasource.ts migration:generate",
    "migration:run": " typeorm-ts-node-commonjs -d orm-datasource.ts migration:run",
    "migration:create": "typeorm-ts-node-commonjs -d orm-datasource.ts migration:create",
    "migration:revert": "typeorm-ts-node-commonjs -d orm-datasource.ts migration:revert"
  },
  "devDependencies": {
    "@nestjs/testing": "^9.4.3",
    "@reapit/config-manager": "workspace:packages/config-manager",
    "@reapit/ts-scripts": "workspace:packages/ts-scripts",
    "@types/adm-zip": "^0.5.5",
    "@types/aws-serverless-express": "^3.3.9",
    "@types/mime-types": "^2.1.4",
    "@types/passport-jwt": "^3.0.13",
    "@types/project-name-generator": "^2.1.3",
    "@types/rimraf": "^3.0.2",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "concurrently": "^6.5.1",
    "cross-env": "^7.0.3",
    "esbuild-plugins-node-modules-polyfill": "^1.6.3",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "mime": "2.6.0",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsup": "^6.7.0",
    "typescript": "^5.4.2"
  },
  "version": "3.0.0"
}
