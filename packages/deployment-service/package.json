{
  "name": "deployment-service",
  "dependencies": {
    "@aws-sdk/client-acm": "^3.670.0",
    "@aws-sdk/client-cloudfront": "^3.675.0",
    "@aws-sdk/client-rds": "^3.709.0",
    "@aws-sdk/client-route-53": "^3.675.0",
    "@aws-sdk/client-secrets-manager": "^3.699.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.5",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.5",
    "@nestjs/microservices": "^11.0.5",
    "@nestjs/passport": "^11.0.4",
    "@nestjs/platform-express": "^11.0.5",
    "@nestjs/typeorm": "^8.1.4",
    "@octokit/app": "^14.0.2",
    "@reapit/foundations-ts-definitions": "^2.1.20",
    "@reapit/utils-authorizer": "workspace:^",
    "@reapit/utils-nest": "workspace:packages/utils-nest",
    "@reapit/utils-node": "workspace:^",
    "adm-zip": "^0.5.16",
    "atlassian-jwt": "^2.0.3",
    "aws-cdk": "^2.162.1",
    "aws-serverless-express": "^3.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "js-yaml": "^4.1.0",
    "jwt-decode": "^4.0.0",
    "mime-types": "^2.1.35",
    "mysql": "^2.18.1",
    "nestjs-typeorm-paginate": "^4.0.4",
    "octokit-plugin-create-pull-request": "^6.0.1",
    "original-fs": "^1.2.0",
    "passport-jwt": "^4.0.1",
    "project-name-generator": "^2.1.9",
    "pusher": "^5.2.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "yaml": "^2.6.0"
  },
  "main": "./src/index.ts",
  "scripts": {
    "start": "NODE_ENV=local nodemon src/local-http.ts",
    "start:sqs": "NODE_ENV=local nodemon src/sqs.ts",
    "start:sns": "NODE_ENV=local nodemon src/sns.ts",
    "test": "cross-env jest --watch --colors",
    "build": "tsup",
    "lint": "eslint --cache --ext=ts,tsx,js src cdk/lib migrations",
    "check": "tsc --noEmit --skipLibCheck",
    "deploy": "rpt-cdk deploy cdk/cdk.ts",
    "release": "echo '...skipping...'",
    "synth": "rpt-cdk synth cdk/cdk.ts",
    "bundle": "tsup-zip deployment-service && zip bundle/resolve-production-s3-bucket-policies.zip dist/resolve-production-s3-bucket-policies.js && zip bundle/resolve-production-s3-bucket-permissions.zip dist/resolve-production-s3-bucket-permissions.js && zip bundle/resolve-production-apply-OAC-to-all-distros.zip dist/resolve-production-apply-OAC-to-all-distros.js",
    "release:watch": "rpt-cdk watch cdk/cdk.ts",
    "release:destroy": "rpt-cdk destroy cdk/cdk.ts",
    "publish": "echo '...skipping...'",
    "conf": "yarn config-manager --namespace cloud --entity deployment-service --name local --mode fetch",
    "migration:generate": "typeorm-ts-node-commonjs -d orm-datasource.ts migration:generate",
    "migration:run": " typeorm-ts-node-commonjs -d orm-datasource.ts migration:run",
    "migration:create": "typeorm-ts-node-commonjs -d orm-datasource.ts migration:create",
    "migration:revert": "typeorm-ts-node-commonjs -d orm-datasource.ts migration:revert"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.0.5",
    "@reapit/config-manager": "workspace:packages/config-manager",
    "@reapit/ts-scripts": "workspace:packages/ts-scripts",
    "@types/adm-zip": "^0.5.5",
    "@types/aws-serverless-express": "^3.3.9",
    "@types/js-yaml": "^4",
    "@types/mime-types": "^2.1.4",
    "@types/passport-jwt": "^3.0.13",
    "@types/project-name-generator": "^2.1.3",
    "@types/rimraf": "^3.0.2",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "concurrently": "^6.5.1",
    "constructs": "^10.4.2",
    "cross-env": "^7.0.3",
    "esbuild-plugins-node-modules-polyfill": "^1.6.7",
    "eslint": "^8.57.1",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "mime": "2.6.0",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.6",
    "typescript": "^5.6.3"
  },
  "version": "3.2.3"
}
