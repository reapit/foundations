{
  "name": "deployment-service",
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.100.0",
    "@aws-sdk/client-route-53": "^3.100.0",
    "@homeservenow/serverless-aws-handler": "^0.2.4",
    "@octokit/app": "^12.0.5",
    "@reapit/api-key-verify": "workspace:packages/api-key-verify",
    "@reapit/connect-session": "workspace:packages/connect-session",
    "@reapit/utils-node": "workspace:packages/utils-node",
    "adm-zip": "^0.5.9",
    "atlassian-jwt": "^2.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "jwt-decode": "^3.1.2",
    "mime-types": "^2.1.35",
    "mysql": "^2.18.1",
    "nestjs-typeorm-paginate": "^2.6.3",
    "node-fetch": "2.6.7",
    "original-fs": "^1.1.0",
    "project-name-generator": "^2.1.9",
    "pusher": "^5.1.0-beta",
    "rimraf": "^3.0.2",
    "typeorm": "^0.2.45",
    "typeorm-aurora-data-api-driver": "^2.4.2",
    "yaml": "^2.1.1"
  },
  "main": "./src/index.ts",
  "scripts": {
    "start": "echo '...skipping...'",
    "test": "cross-env jest --watch --colors",
    "build": "tsc -p .",
    "lint": "eslint --cache --ext=ts,tsx,js src cdk/lib",
    "check": "tsc --noEmit --skipLibCheck",
    "deploy": "rpt-cdk deploy cdk/cdk.ts",
    "synth": "rpt-cdk synth cdk/cdk.ts",
    "bundle": "ts-bundler",
    "release": "yarn deploy",
    "release:watch": "rpt-cdk watch cdk/cdk.ts",
    "release:destroy": "rpt-cdk destroy cdk/cdk.ts",
    "publish": "echo '...skipping...'",
    "conf": "yarn config-manager --namespace cloud --entity deployment-service --name local --mode fetch",
    "migration:generate": "ts-node -P ./tsconfig.json ../../node_modules/.bin/typeorm migration:generate",
    "migration:run": "ts-node -P ./tsconfig.json ../../node_modules/.bin/typeorm migration:run",
    "migration:create": "ts-node -P ./tsconfig.json ../../node_modules/.bin/typeorm migration:create",
    "migration:revert": "ts-node -P ./tsconfig.json ../../node_modules/.bin/typeorm migration:revert"
  },
  "devDependencies": {
    "@reapit/config-manager": "workspace:packages/config-manager",
    "@reapit/ts-bundler": "workspace:packages/ts-bundler",
    "@reapit/ts-scripts": "workspace:packages/ts-scripts",
    "@types/adm-zip": "^0.5.0",
    "@types/mime-types": "^2.1.1",
    "@types/project-name-generator": "^2.1.1",
    "@types/rimraf": "^3.0.2",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "concurrently": "^6.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.16.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.0",
    "mime": "2.6.0",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  }
}
