{
  "name": "api-key-service",
  "dependencies": {
    "@aws/dynamodb-data-mapper-annotations": "^0.7.3",
    "@homeservenow/serverless-aws-handler": "^0.2.4",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1"
  },
  "devDependencies": {
    "@reapit/config-manager": "workspace:packages/config-manager",
    "@reapit/foundations-ts-definitions": "workspace:packages/foundations-ts-definitions",
    "@reapit/ts-scripts": "workspace:packages/ts-scripts",
    "@reapit/utils": "workspace:packages/utils",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.27.0",
    "jest": "26.6.3",
    "prettier": "^2.3.0",
    "serverless": "^2.43.1"
  },
  "scripts": {
    "start": "yarn publicKeyFetch && yarn build && yarn serverless offline start --runtime nodejs12.x --stage local",
    "build": "echo '...skipping...'",
    "lint": "concurrently \"prettier src/**/*.ts --write \"  \"eslint --cache --ext=ts,tsx src --fix\"",
    "test": "yarn publicKeyFetch --test && cross-env TZ=UTC jest --colors --watch --silent",
    "conf": "yarn config-manager --namespace cloud --entity api-key-service --name local --mode fetch",
    "release": "yarn publicKeyFetch && serverless deploy",
    "publish": "echo '...skipping...'",
    "publicKeyFetch": "reapit-fetch-public-keys"
  }
