{
  "name": "@reapit/cli",
  "version": "2.1.2",
  "description": "A cli tool for reapit cloud",
  "main": "dist/index.js",
  "dependencies": {
    "adm-zip": "^0.5.9",
    "ansi-regex": "^6.0.1",
    "axios": "^0.27.2",
    "chalk": "^4.1.2",
    "cli-spinners": "^2.7.0",
    "create-react-app": "^5.0.1",
    "figlet": "^1.5.2",
    "inquirer": "^8.2.4",
    "latest-semver": "^3.0.0",
    "nestjs-typeorm-paginate": "^2.6.3",
    "ora": "^5.4.1",
    "process.argv": "^0.6.0",
    "pusher-js": "^7.3.0",
    "reflect-metadata": "^0.1.13",
    "semver": "^7.3.7",
    "shleemy": "^1.2.2",
    "simple-git": "^3.10.0"
  },
  "bin": {
    "reapit": "./dist/cli/src/index.js"
  },
  "repository": {
    "url": "https://github.com/reapit/foundations/packages/cli",
    "type": "git"
  },
  "devDependencies": {
    "@reapit/config-manager": "workspace:packages/config-manager",
    "@reapit/ts-scripts": "workspace:packages/ts-scripts",
    "@reapit/ts-types": "workspace:packages/ts-types",
    "@types/adm-zip": "^0.5.0",
    "@types/figlet": "^1.5.4",
    "@types/inquirer": "^7.3.3",
    "@types/semver": "^7.3.10",
    "@types/yargs": "^16.0.4",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "babel-jest": "^28.1.3",
    "concurrently": "^6.5.1",
    "eslint": "^8.20.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1"
  },
  "scripts": {
    "run": "ts-node src/index.ts",
    "build": "tsc -p .",
    "lint": "eslint --cache --ext=ts,tsx,js src",
    "check": "tsc --noEmit --skipLibCheck",
    "conf": "yarn config-manager --namespace cloud --entity cli --name local --mode fetch",
    "test": "jest --watch --colors",
    "release": "echo '...skipping...'",
    "publish": "yarn npm publish --access public"
  }
}
