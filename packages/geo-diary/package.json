  "main": "./src/index.ts",
  "scripts": {
    "build:prod": "webpack --color --mode production --config ../../scripts/webpack/webpack.sass.prod.js",
    "start:dev": "webpack-dev-server --hot --progress --color --mode development --config ../../scripts/webpack/webpack.sass.dev.js",
