{
  "name": "geo-diary",
  "version": "4.2.2",
  "private": true,
  "description": "Reapit Geo Diary App",
  "repository": {
    "type": "git",
    "url": "git+git@github.com:reapit/app-name.git"
  },
  "license": "MIT",
  "author": "Reapit",
  "main": "./src/index.ts",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "cross-env TZ=UTC jest --watch --colors --silent",
    "lint": "eslint --cache --ext=ts,tsx,js src",
    "check": "tsc --noEmit --skipLibCheck",
    "release": "echo '...skipping...'",
    "deploy": "rpt-cdk deploy",
    "publish": "echo '...skipping...'",
    "conf": "yarn config-manager --namespace cloud --entity geo-diary --name local --mode fetch",
    "commit": "yarn test --coverage --no-cache --silent --forceExit --detectOpenHandles --runInBand --watch=false && jest-coverage-badges --input src/tests/coverage/coverage-summary.json --output src/tests/badges && yarn lint --fix && yarn check"
  },
  "dependencies": {
    "@apollo/client": "^3.7.14",
    "@linaria/core": "^4.2.10",
    "@linaria/react": "^4.3.8",
    "@reapit/connect-session": "workspace:packages/connect-session",
    "@reapit/elements": "workspace:packages/elements",
    "@reapit/foundations-ts-definitions": "workspace:packages/foundations-ts-definitions",
    "@reapit/utils-common": "workspace:packages/utils-common",
    "@sentry/browser": "^7.52.1",
    "dayjs": "^1.11.7",
    "graphql": "15.8.0",
    "little-loader": "^0.2.0",
    "lodash.debounce": "^4.0.8",
    "query-string": "5.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-map": "^3.1.1",
    "react-hook-form": "^7.43.9",
    "react-icons": "^4.7.1",
    "react-router": "^6.11.2",
    "react-router-dom": "^6.11.2",
    "react-swipeable": "^6.2.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@apollo/react-testing": "^4.0.0",
    "@linaria/shaker": "^4.2.11",
    "@reapit/config-manager": "workspace:packages/config-manager",
    "@reapit/ts-scripts": "workspace:packages/ts-scripts",
    "@reapit/ts-types": "workspace:packages/ts-types",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "concurrently": "^6.5.1",
    "cross-env": "^6.0.3",
    "eslint": "8.35.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-coverage-badges": "^1.1.2",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5",
    "vite": "^4.3.8"
  }
}
