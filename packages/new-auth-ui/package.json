{
  "name": "new-auth-ui",
  "version": "1.0.0",
  "description": "An example UI for authentication with several methods",
  "main": "./src/index.ts",
  "repository": "git@github.com:reapit/app-store.git",
  "author": "Will McVay <wmcvay@reapit.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "echo '...skipping..'",
    "lint": "eslint --cache --ext=ts,tsx,js src",
    "check": "tsc --noEmit --skipLibCheck",
    "deploy": "rpt-cdk deploy",
    "release": "echo '...skipping...'",
    "synth": "rpt-cdk synth",
    "publish": "echo '...skipping...'",
    "conf": "yarn config-manager --namespace cloud --entity new-auth-ui --name local --mode fetch",
    "commit": "yarn test --coverage --no-cache --silent --forceExit --detectOpenHandles --runInBand --watch=false && jest-coverage-badges --input src/tests/coverage/coverage-summary.json --output src/tests/badges && yarn lint --fix && yarn check"
  },
  "dependencies": {
    "@harelpls/use-pusher": "^7.2.1",
    "@hookform/resolvers": "^2.9.11",
    "@linaria/core": "^4.2.8",
    "@linaria/react": "^4.3.6",
    "@reapit/connect-session-next": "workspace:packages/connect-session-next",
    "@reapit/elements": "workspace:packages/elements",
    "@reapit/utils-common": "workspace:packages/utils-common",
    "@reapit/utils-react": "workspace:packages/utils-react",
    "@sentry/browser": "^7.48.0",
    "@sentry/react": "^7.48.0",
    "@sentry/tracing": "^7.48.0",
    "amazon-cognito-identity-js": "^6.2.0",
    "chart.js": "^3.9.1",
    "dayjs": "^1.11.7",
    "diff": "^4.0.2",
    "file-saver": "^2.0.5",
    "github-username-regex": "^1.0.0",
    "papaparse": "^5.3.2",
    "project-name-generator": "^2.1.9",
    "react": "^18.2.0",
    "react-chartjs-2": "^4.3.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-router": "^6.10.0",
    "react-router-dom": "^6.10.0",
    "snyk": "^1.1110.0",
    "swagger-ui-react": "^4.17.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@linaria/shaker": "^4.2.9",
    "@reapit/config-manager": "workspace:packages/config-manager",
    "@reapit/foundations-ts-definitions": "workspace:packages/foundations-ts-definitions",
    "@reapit/ts-scripts": "workspace:packages/ts-scripts",
    "@reapit/ts-types": "workspace:packages/ts-types",
    "@testing-library/react": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "concurrently": "^6.5.1",
    "cross-env": "^6.0.3",
    "css-loader": "^6.7.3",
    "eslint": "8.35.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-coverage-badges": "^1.1.2",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "rimraf": "^3.0.2",
    "serverless": "^2.72.3",
    "serverless-s3-deploy": "^0.10.1",
    "serverless-s3-remover": "^0.6.0",
    "serverless-single-page-app-plugin": "^1.0.4",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5",
    "vite": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test --ci --no-color"
    }
  },
  "lint-staged": {
    "!(**/cypress/**/*.{ts,tsx})/**/src/**/*.{ts,tsx}": [
      "yarn lint:src",
      "git add"
    ]
  },
  "browserslist": [
    "> 0.5%",
    "not IE 11",
    "chrome 69"
  ],
  "snyk": true
}
