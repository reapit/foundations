{
  "name": "graphql-server-v2",
  "version": "2.0.1",
  "private": true,
  "description": "GraphQL Server",
  "repository": "https://github.com/reapit/foundations.git",
  "license": "MIT",
  "author": "reapit global",
  "main": "dist/index.js",
  "scripts": {
    "start": "PLATFORM_API_BASE_URL=https://platform.dev.paas.reapit.cloud ts-node-dev -r tsconfig-paths/register src/http-local",
    "test": "jest --watch --colors --silent",
    "build": "tsc -p .",
    "bundle": "ts-bundler",
    "lint": "eslint --cache --ext=ts,js src",
    "check": "echo '...skipping...'",
    "publish": "echo '...skipping...'",
    "conf": "yarn config-manager --namespace cloud --entity graphql-server-v2 --name local --mode fetch",
    "commit": "yarn test --coverage --no-cache --silent --forceExit --detectOpenHandles --runInBand --watch=false && jest-coverage-badges --input src/tests/coverage/coverage-summary.json --output src/tests/badges && yarn lint --fix && yarn check",
    "prepublish": "npm run snyk-protect",
    "snyk-protect": "snyk-protect",
    "deploy": "rpt-cdk deploy",
    "release": "echo '...skipping...'",
    "synth": "rpt-cdk synth",
    "download:schema": "ts-node src/download-swagger.ts"
  },
  "dependencies": {
    "@sentry/node": "^7.107.0",
    "aws-lambda": "^1.0.7",
    "aws-serverless-express": "^3.4.0",
    "axios": "^1.6.8",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "express-graphql": "^0.12.0",
    "express-graphql-header": "^1.0.1",
    "graphql": "^15.8.0",
    "isomorphic-fetch": "^3.0.0",
    "swagger-to-graphql": "^4.0.2"
  },
  "devDependencies": {
    "@reapit/config-manager": "workspace:packages/config-manager",
    "@reapit/ts-bundler": "workspace:packages/ts-bundler",
    "@reapit/ts-scripts": "workspace:packages/ts-scripts",
    "@types/aws-serverless-express": "^3.3.9",
    "@types/graphql": "^14.5.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "aws-cdk-lib": "^2.133.0",
    "axios-mock-adapter": "^1.22.0",
    "bufferutil": "^4.0.8",
    "concurrently": "^6.5.1",
    "cross-env": "^7.0.3",
    "eslint": "8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "graphql-tag": "^2.12.6",
    "jest": "^29.7.0",
    "jest-coverage-badges": "^1.1.2",
    "prettier": "^3.2.5",
    "supertest-graphql": "^1.1.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^3.15.0",
    "typescript": "^5.4.2"
  },
  "resolutions": {
    "logform": "2.3.0"
  },
  "snyk": true
}
