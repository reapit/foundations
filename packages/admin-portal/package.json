    "@reapit/elements-legacy": "workspace:packages/elements-legacy",
    "@reapit/utils": "workspace:packages/utils",
    "@sentry/browser": "^6.9.0",
    "chart.js": "^2.9.4",
    "dayjs": "^1.10.6",
    "diff": "^4.0.2",
    "lodash.isequal": "^4.5.0",
    "lodash.orderby": "^4.6.0",
    "query-string": "5.1.1",
