{
  "name": "payments-client",
  "description": "Payments Client For Sage Pay",
  "version": "4.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+git@github.com:reapit/foundations.git"
  },
  "license": "MIT",
  "author": "Author",
  "main": "./src/index.ts",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "cross-env TZ=UTC jest --watch --colors --silent",
    "lint": "eslint --cache --ext=ts,tsx,js src",
    "check": "tsc --noEmit --skipLibCheck",
    "synth": "rpt-cdk synth",
    "deploy": "rpt-cdk deploy",
    "release": "echo '...skipping...'",
    "publish": "echo '...skipping...'",
    "conf": "yarn config-manager --namespace cloud --entity payments-client --name local --mode fetch",
    "commit": "yarn test --coverage --no-cache --silent --forceExit --detectOpenHandles --runInBand --watch=false && jest-coverage-badges --input src/tests/coverage/coverage-summary.json --output src/tests/badges && yarn lint --fix && yarn check"
  },
  "dependencies": {
    "@linaria/core": "^5.0.2",
    "@linaria/react": "^5.0.3",
    "@reapit/connect-session": "workspace:packages/connect-session",
    "@reapit/elements": "^4.0.4",
    "@reapit/payments-ui": "workspace:packages/payments-ui",
    "@reapit/use-reapit-data": "workspace:packages/use-reapit-data",
    "@reapit/utils-common": "workspace:packages/utils-common",
    "@sentry/browser": "^7.84.0",
    "@sentry/react": "^7.84.0",
    "@sentry/tracing": "^7.84.0",
    "dayjs": "^1.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-router": "^6.20.0",
    "react-router-dom": "^6.20.0",
    "snyk": "^1.1259.0",
    "swr": "^1.3.0",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@linaria/shaker": "^5.0.3",
    "@reapit/config-manager": "workspace:packages/config-manager",
    "@reapit/ts-scripts": "workspace:packages/ts-scripts",
    "@reapit/ts-types": "workspace:packages/ts-types",
    "@testing-library/react": "^14.1.2",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "concurrently": "^6.5.1",
    "cross-env": "^6.0.3",
    "eslint": "8.56.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-coverage-badges": "^1.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  }
}
