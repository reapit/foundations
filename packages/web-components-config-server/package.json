{
  "name": "web-components-config-server",
  "version": "0.0.0",
  "private": true,
  "description": "GraphQL Server",
  "repository": "https://github.com/reapit/foundations.git",
  "license": "MIT",
  "author": "reapit global",
  "main": "dist/index.js",
  "scripts": {
    "lint": "concurrently \"tsc --noEmit\" \"eslint --cache --ext=ts,js src\"",
    "lint:fix": "eslint --cache --ext=ts,js src --fix",
    "test:ci": "cross-env TZ=UTC jest --ci --colors --coverage --silent --forceExit --detectOpenHandles --runInBand",
    "test:dev": "cross-env TZ=UTC jest --watch --verbose",
    "start:dev": "yarn build:credentials && NODE_ENV=development SLS_DEBUG=* serverless offline --verbose",
    "build:serverless": "yarn build:credentials && serverless webpack --out dist --stage dev",
    "release:dev": "yarn build:credentials && serverless deploy --stage dev",
    "release:development": "yarn build:credentials &&  serverless deploy --stage dev",
    "release:production": "yarn build:credentials && serverless deploy --stage prod",
    "fetch-config": "yarn config-manager --namespace cloud --entity web-components-config-server --name local --mode fetch",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect",
    "build:credentials": "node ./scripts/load-credentials.js"
  },
  "dependencies": {
    "@aws/dynamodb-data-mapper": "^0.7.3",
    "@aws/dynamodb-data-mapper-annotations": "^0.7.3",
    "async": "^3.2.0",
    "aws-sdk": "^2.682.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "color-convert": "^2.0.1",
    "color-name": "^1.1.4",
    "colors": "^1.4.0",
    "express": "^4.17.1",
    "is-stream": "^2.0.0",
    "morgan": "^1.10.0",
    "ms": "^2.1.2",
    "mysql2": "^2.1.0",
    "readable-stream": "^3.6.0",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1",
    "snyk": "^1.341.1",
    "stack-trace": "^0.0.10",
    "uuid": "^3.3.3",
    "winston": "^3.2.1",
    "@reapit/foundations-ts-definitions": "2020-03-04"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@types/jest": "^25.1.2",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.13.2",
    "@types/uuid": "^3.4.6",
    "babel-loader": "^8.0.6",
    "babel-runtime": "^6.26.0",
    "bufferutil": "^4.0.1",
    "jest": "^25.1.0",
    "json-loader": "^0.5.7",
    "serverless-http": "^2.3.2",
    "serverless-offline": "^6.1.4",
    "serverless-webpack": "^5.3.1",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "tsdx": "^0.12.3",
    "typescript": "^3.8.3",
    "utf-8-validate": "^5.0.2",
    "webpack-node-externals": "^1.7.2"
  },
  "snyk": true
}
