{
  "name": "web-components-config-server",
  "version": "0.0.0",
  "private": true,
  "description": "GraphQL Server",
  "repository": "https://github.com/reapit/foundations.git",
  "license": "MIT",
  "author": "reapit global",
  "main": "dist/index.js",
  "scripts": {
    "build:prod": "cross-env NODE_ENV=production rimraf dist && tsc --p tsconfig.json",
    "build:serverless": "serverless webpack --out dist --stage dev",
    "lint": "concurrently \"tsc --noEmit\" \"eslint --cache --ext=ts,js src\"",
    "lint:fix": "eslint --cache --ext=ts,js src --fix",
    "release:dev": "serverless deploy --stage dev",
    "release:prod": "node ../../scripts/release/release-serverless.js web-components-config-server",
    "start:dev": "cross-env NODE_ENV=development nodemon",
    "start:prod": "node ./dist/app.js",
    "test:ci": "cross-env TZ=UTC jest --ci --colors --coverage --silent --forceExit",
    "test:dev": "cross-env TZ=UTC jest --watch --verbose",
    "fetch-config": "yarn config-manager fetchConfig web-components-config-server"
  },
  "dependencies": {
    "@reapit/cognito-auth": "^2.0.19",
    "async": "^3.2.0",
    "aws-serverless-express": "^3.3.8",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "color-convert": "^2.0.1",
    "color-name": "^1.1.4",
    "colors": "^1.4.0",
    "express": "^4.17.1",
    "is-stream": "^2.0.0",
    "morgan": "^1.10.0",
    "ms": "^2.1.2",
    "mysql2": "^2.1.0",
    "readable-stream": "^3.6.0",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1",
    "serverless": "^1.64.1",
    "stack-trace": "^0.0.10",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.2",
    "@types/uuid": "^3.4.6",
    "babel-loader": "^8.0.6",
    "babel-runtime": "^6.26.0",
    "bufferutil": "^4.0.1",
    "copy-webpack-plugin": "^5.1.1",
    "jest": "^25.1.0",
    "json-loader": "^0.5.7",
    "nodemon": "^2.0.2",
    "serverless-webpack": "^5.3.1",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "tsdx": "^0.12.3",
    "utf-8-validate": "^5.0.2",
    "webpack-node-externals": "^1.7.2"
  }
}
