    "check:precommit": "lerna run check:fix --parallel --stream --since HEAD",
    "check:all": "lerna run check:fix --parallel --stream --include-dependencies --",
    "release:development": "lerna run release:development --parallel",
    "upload-artifact": "node ./scripts/release/upload-artifact.js",
    "release": "node ./scripts/release/release.js",
    "fetch-artifact": "node ./scripts/release/fetch-artifact.js",
    "test": "lerna run test:ci --parallel",
    "test-e2e:ci": "lerna run test-e2e:ci --stream",
    "scan-secrets": "node --unhandled-rejections=strict ./scripts/utils/git-secrets.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn scan-secrets && yarn check:precommit && lerna run test:update-badges --stream --parallel --since HEAD && git add .",
